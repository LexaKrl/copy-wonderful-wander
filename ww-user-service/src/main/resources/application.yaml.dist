# Example configuration file for sensitive data
spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/your-database
    driver-class-name: your.Driver.ClassName
    username: your-username
    password: your-password
    hikari:
      maximum-pool-size: 20

jwt:
  access-token-duration: 3_600_000    # 1000 * 60 * 60 (1 час)
  refresh-token-duration: 86_400_00   # 1000 * 60 * 60 * 24 (1 день)
  time-token-end-soon: 900_000        # 1000 * 60 * 15 (15 минут)
  secret-key: "example-key"
